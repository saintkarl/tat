<!-- Default decorators, you can let this file empty by don't remove it -->
<decorators defaultdir="/decorators">
    <excludes>
        <pattern>/40*.jsp</pattern>
        <pattern>/*ajax=true*</pattern>
        <pattern>/ajax/*</pattern>
        <pattern>/logout*</pattern>
        <pattern>/scripts/**</pattern>
        <pattern>/ckeditor442/**</pattern>
        <pattern>/*/login.*</pattern>
        <pattern>/mobile/**</pattern>
        <pattern>/api/**</pattern>
    </excludes>

    <decorator name="rlwebsite_login" page="rllogin.jsp">
        <pattern>/rlwebsite/index*</pattern>
        <pattern>/rlwebsite/feedback*</pattern>
    </decorator>

    <decorator name="rlwebsite" page="rlhomepage.jsp">
        <pattern>/rlwebsite/*/*</pattern>

    </decorator>

    <!--<decorator name="default" page="default_3.jsp">-->
        <!--<pattern>/*</pattern>-->
    <!--</decorator>-->

    <decorator name="default" page="default_ace.jsp">
        <pattern>/*</pattern>
    </decorator>



    <decorator name="login" page="login_2.jsp">
        <pattern>/login*</pattern>
        <pattern>/forgotpwd*</pattern>
    </decorator>

</decorators>
